// Preferences Modal
#prefModal.modal.fade(style="color:black;overflow-y:auto;z-index:2;")
  .modal-dialog
    .modal-content
      #modalNav
        h3#opt_title Settings
        // Nav tabs
        ul#opt_tabs.nav.nav-pills.nav-stacked
          li
            a#optNav_general(href="#tabContentGeneral", name="general", data-toggle="pill") General
          li
            a#optNav_autologin(href="#tabContentAutologin", name="autologin", data-toggle="pill") Auto login
          li
            a#optNav_blacklist(href="#tabContentBlacklist", name="blacklist", data-toggle="pill") Blacklist
          li
            a#optNav_extensions(href="#tabContentExtensions", name="extensions", data-toggle="pill") Extensions
          li
            a#optNav_about(href="#tabContentAbout", name="about", data-toggle="pill") About
        button#opt_reset.btn.btn-default(type="button")
      h3#modalHeader General
      button.close(type="button", data-dismiss="modal", aria-hidden="true") &#215;
      .modal-body
        // Tab panes
        .tab-content
          #tabContentGeneral.tab-pane
            .group
              .list-group
                .list-group-item-text
                  #opt_openInPackagedApp.checkbox
                  #opt_closeQuery.checkbox
                  #opt_enablePicPreview.checkbox
                  #opt_enableNotifications.checkbox
                  #opt_enableEasyReading.checkbox
                  #opt_endTurnsOnLiveUpdate.checkbox
                  #opt_copyOnSelect.checkbox
                  #opt_antiIdleTime.form-group
                  #opt_lineWrap.form-group
              .list-group
                h4#opt_appearance.list-group-item-heading
                .list-group-item-text
                  #opt_fontFitWindowWidth.checkbox
                  #opt_fontFace.form-group
                  #opt_bbsMargin.form-group
            .group
              .list-group
                h4#opt_mouseBrowsing.list-group-item-heading
                .list-group-item-text
                  #opt_useMouseBrowsing.checkbox
                  #opt_mouseBrowsingHighlight.checkbox
                  #opt_mouseBrowsingHighlightColor
                  #opt_mouseLeftFunction.form-group
                  #opt_mouseMiddleFunction.form-group
                  #opt_mouseWheelFunction1.form-group
                  #opt_mouseWheelFunction2.form-group
                  #opt_mouseWheelFunction3.form-group
          #tabContentAutologin.tab-pane
            .group
              small#opt_autologinWarning(style="color:red;")
              .list-group
                h4#opt_autologin.list-group-item-heading(style="margin-bottom:10px;")
                .list-group-item-text
                  #login_username.form-group
                  #login_password.form-group
                #opt_enableDeleteDupLogin.checkbox
                .btn-group(data-toggle="buttons", style="margin-left:20px;")
                  label.btn.btn-default
                    input#opt_deleteDupLoginYes(type="radio", name="options")
                    | Y
                  label.btn.btn-default.active
                    input#opt_deleteDupLoginNo(type="radio", name="options", checked="")
                    | N
          #tabContentBlacklist.tab-pane
            #opt_enableBlacklist.checkbox
            #blacklist_driveSync(style="margin-bottom:10px")
              span#blacklist_driveSyncTitle(style="margin-right:5px") Use Google Drive:
              a#blacklist_driveAuthorize.btn.btn-default(style="margin-top:-3px") Authorize PttChrome
              a#blacklist_driveLoad.btn.btn-default(style="margin-top:-3px") Load
              .btn-group(style="margin-top:-3px")
                button#blacklist_driveSave.btn.btn-default.dropdown-toggle(type="button", data-toggle="dropdown")
                ul.dropdown-menu(role="menu")
                  li
                    a#blacklist_driveSaveNew(style="padding:3px 20px; cursor:pointer;") New Document
                  li
                    a#blacklist_driveSaveExisting(style="padding:3px 20px; cursor:pointer;") Existing Document
              span#blacklist_driveDone(style="margin-left:5px; display:none;") Done!
              span#blacklist_driveLoading(style="margin-left:5px; display:none;") Loading ...
            span#opt_blacklistInstruction
            // blacklist item start
            textarea#opt_blacklistedUsers.form-control
            // blacklist item end
          #tabContentExtensions.tab-pane
            .group
              .list-group
                h4#ext_quickSearch.list-group-item-heading
                .list-group-item-text(style="margin-top:15px;")
                  ul#ext_quickSearchList.form-group(style="padding:0px")
          #tabContentAbout.tab-pane
            .list-group
              h4.list-group-item-heading
                | PttChrome
                small
                  | by
                  a(href="mailto:yangchuck@gmail.com", target="_blank", rel="noreferrer") Chuck Yang
              a(href="https://github.com/iamchucky/PttChrome", target="_blank", rel="noreferrer") https://github.com/iamchucky/PttChrome
              .list-group-item-text(style="margin-top:10px;")
                a#about_review.btn.btn-default(href="https://chrome.google.com/webstore/detail/pttchrome/hhnlfapopmaimdlldbknjdgekpgffmbo/reviews", target="_blank", rel="noreferrer")
                a#about_feedback.btn.btn-default(href="mailto:pttchrome.feedback@gmail.com", target="_blank", rel="noreferrer")
                a#about_fbpage.btn.btn-default(href="https://www.facebook.com/pttchrome", target="_blank", rel="noreferrer")
            .list-group
              span#about_promote(style="color:red;")
              a(href="https://chrome.google.com/webstore/detail/web2pttchrome/pjemnpgdmnlkkcpaddlnlegmdfpohnep", target="_blank", rel="noreferrer") Web2PttChrome
            .list-group
              h4#about_version_title.list-group-item-heading
              .list-group-item-text
                #about_version
            .list-group
              h4#about_new_title.list-group-item-heading
              .list-group-item-text
                ul#about_new
